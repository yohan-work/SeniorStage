{
	"rewrites": [
	  {
		"source": "/api/(.*)",
		"destination": "/senior-stage-backend/api/$1"
	  },
	  {
		"source": "/(.*)",
		"destination": "/senior-stage/index.html"
	  }
	],
	"cleanUrls": true,
	"trailingSlash": false,
	"redirects": [
	  {
		"source": "/index",
		"destination": "/",
		"permanent": true
	  }
	]
  }
  